{
    "name": "lunar-banner/addon",
    "version": "0.1",
    "type": "project",
    "description": "Banner functionality for Lunar.",
    "keywords": [
        "lunarphp",
        "laravel",
        "ecommerce",
        "e-commerce",
        "headless",
        "banner package for lunar",
        "banner addon for lunar"
    ],
    "license": "MIT",
    "source": {
        "type": "git",
        "url": "https://github.com/SteliyanPStoyanov/lunar-banner-addon.git",
        "reference": ""
    },
    "authors": [
        {
            "name": "Steliyan Stoyanov",
            "homepage": "https://github.com/SteliyanPStoyanov"
        }
    ],
    "require": {
        "php": "^8.0",
        "lunarphp/lunar": "^0.6",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lunar\\Banner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "lunar": {
            "name": "Banner"
        },
        "laravel": {
            "providers": [
                "Lunar\\Banner\\BannerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
